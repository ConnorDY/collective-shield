{
  "name": "maskshield",
  "version": "0.1.0",
  "proxy": "http://localhost:3050",
  "private": true,
  "scripts": {
    "start": "ts-node --project server/tsconfig.json server/app.ts",
    "heroku-postbuild": "npm run build",
    "start-dev": "env BROWSER=false run-p start-server start-client",
    "start-client": "react-scripts start",
    "start-server": "ts-node-dev --project server/tsconfig.json server/app.ts",
    "prod": "node ./api/app.js",
    "build": "react-scripts build",
    "build:api": "tsc --project server/tsconfig.json",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --runInBand=true",
    "eject": "react-scripts eject",
    "prettier:base": "prettier \"{src,server}/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "prettier:check": "npm run prettier:base -- --check",
    "prettier:write": "npm run prettier:base -- --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "celebrate": "^12.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "heroku-ssl-redirect": "0.0.4",
    "http-proxy-middleware": "^1.0.3",
    "input-moment": "^0.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.6",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.13.1",
    "react-avatar": "^3.9.2",
    "react-bootstrap": "^1.0.0",
    "react-color": "^2.18.0",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.7.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.1-alpha.2",
    "sparkpost": "^2.1.4",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/enzyme": "^3.10.5",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.8",
    "@types/multer": "^1.4.2",
    "@types/passport": "^1.0.3",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-modal": "^3.10.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/sparkpost": "^2.1.4",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "node-dev": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,server}/**/*.{js,jsx,ts,tsx,css,scss}": [
      "npm run prettier:write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "none"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text-summary",
      "lcov"
    ]
  }
}
